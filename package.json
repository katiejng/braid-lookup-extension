{
  "name": "sku-extension",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "sku start",
    "test": "sku test",
    "build": "SKU_TELEMETRY=false && sku build",
    "serve": "sku serve",
    "lint": "sku lint",
    "format": "sku format",
    "build-zip": "yarn build && yarn zip:chrome && yarn zip:firefox",
    "set-version": ".github/scripts/generate_manifest.sh",
    "zip:chrome": "zip -r packaged/chrome.zip dist",
    "zip:firefox": "yarn zip:source && cd dist; zip -r ../packaged/firefox.zip *",
    "zip:source": "git archive HEAD -o packaged/sourceCode.zip",
    "web-ext": "web-ext"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sku pre-commit"
    }
  },
  "dependencies": {
    "braid-design-system": "28.3.0",
    "dotenv": "^8.2.0",
    "fuse.js": "6.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sku": "10.3.6"
  },
  "devDependencies": {
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "chrome-webstore-upload": "0.2.2",
    "husky": "4.2.5",
    "web-ext": "^4.2.0"
  }
}
